{
  "name": "nextra",
  "scripts": {
    "private-route-gen": "ts-node -e \"import gen from './src/generatePrivateRoutes'; gen.changeMiddleware()\"",
    "dev": "npm run private-route-gen && next",
    "prebuild": "npm run private-route-gen",
    "build": "next build",
    "start": "next start",
    "e2e:test": "npx playwright test",
    "e2e:test:show-report": "npx monocart show-report ./e2e-test-results/monocart-report.html",
    "test": "jest --runInBand",
    "types:check": "tsc --noEmit",
    "link:check": "pnpm run contentlayer && node ./scripts/link-check.mjs",
    "contentlayer": "contentlayer build --verbose"
  },
  "dependencies": {
    "next": "^14.2.4",
    "next-auth": "5.0.0-beta.19",
    "next-compose-plugins": "^2.2.1",
    "nextra": "^2.13.4",
    "nextra-theme-docs": "^2.13.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.5",
    "autoprefixer": "^10.4.19",
    "contentlayer": "^0.3.4",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdown-link-check": "^3.12.2",
    "monocart-reporter": "^2.5.0",
    "next-contentlayer": "^0.3.4",
    "open-cli": "^8.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5"
  }
}
